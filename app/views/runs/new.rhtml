<h1>Setup run</h1>

<p><%= error_messages_for :run %></p>

<% form_for(:run, :url => runs_path, :html => {:multipart => true}) do |f| %>
<p>
	<label for="file_name">Setup file</label>
	<%= f.file_field :file %>
</p>
<p>
	<label for="run_comment">Comment</label>
	<%= f.text_area :comment, "rows" => 3 %>
</p>
<%= f.submit 'Create', :disable_with => "Creating&hellip;" %>
<% end %>

<%= link_to 'Back', runs_path %>
